package io.sarl.api.protocol.examples.prescription.pharmacist

import io.sarl.api.protocol.examples.prescription.events.Prescribe

/** 
 * Patient -> any Doctor : Request [ in id, out symptoms, out age]
 * Doctor -> any Pharmacist : Prescribe [ in id, in symptoms, in age, out prescription]
 */
behavior PharmacistBehavior {

	uses PharmacistProtocolCapacity, PharmacistInnerCapacity
	
	on Prescribe {
		var pkg = preparePkg(occurrence.rx)
		setPkg(pkg)
	}

}
