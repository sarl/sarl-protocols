package io.sarl.api.protocol

import io.sarl.api.protocol.ProtocolRole
import io.sarl.lang.core.EventSpace
import io.sarl.lang.core.SpaceID
import io.sarl.lang.core.SpaceSpecification

capacity ProtocolEnactment {

	def enact(protocol : Class<? extends SpaceSpecification<S>>, role : ProtocolRole) : S with S extends EventSpace
	
	def enact(spaceId : SpaceID, role : ProtocolRole) : EventSpace
	
	def leave(protocol : EventSpace, role : ProtocolRole) : void

}
